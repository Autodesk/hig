{
  "source": [
    "properties/basics/*.json",
    "themes/light-gray.json",
    "themes/medium-density.json",
    "properties/components/*.json"
  ],
  "platforms": {
    "light-gray-medium-density": {
      "transformGroup": "web",
      "buildPath": "build/js/",
      "files": [{
        "name": "__style_properties",
        "destination": "style-dictionary-properties.js",
        "format": "javascript/object"
      }]
    }
  }
}
